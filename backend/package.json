{
  "name": "free-games-claimer-backend",
  "version": "1.0.0",
  "description": "Free Games Claimer - Collect free games from Epic, GOG, Steam, Prime Gaming",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "bot": "node src/telegram/bot.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "jest --testMatch='**/*.e2e.test.js'",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "express": "^4.18.2",
    "jsonwebtoken": "^9.1.0",
    "bcrypt": "^5.1.0",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3",
    "knex": "^3.0.0",
    "pg": "^8.11.2",
    "redis": "^4.6.10",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "telegraf": "^4.14.1",
    "winston": "^3.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.0",
    "csurf": "^1.11.0",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "nodemailer": "^6.9.7",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "@testing-library/react": "^14.1.2",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "jest-extended": "^4.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
